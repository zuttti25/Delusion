  <div class="row">
 <% if my_like.blank? %>
 
    お気に入りはまだありません
 
  <% else %>
  
    <% my_like.each do |like| %>
    <div class="col-3">
      <%= link_to post_path(like.post) do %>
      <%= attachment_image_tag like.post, :image, format: 'jpeg', size: "200x125", fallback:"no_image.post.jpg" %> <br>
      <% end %>
      <p><%= like.post.title %></p>
      <p>いいね件数: <%= like.post.likes.count %></p>
      <p><%= link_to "#{like.post.comments.count} コメント", post_path(like.post.id) %></p>
      <% like.post.tags.each do |tag| %>
      <p><%= link_to tag.tag_name, posts_path(tag_id: tag.id) %></p>
      <% end %>
    </div>
    <% end %>
    <% end %>
  
  </div>